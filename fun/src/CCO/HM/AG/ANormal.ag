-------------------------------------------------------------------------------
-- AST
-------------------------------------------------------------------------------

data AVal
  | ANat   i :: {Int}
  | AVar   x :: {Var}

data AExp
  | AVal   v :: AVal
  | ABool  b :: {Bool}
  | AApp   e :: AExp   v :: AVal
  | AIf    c :: AVal   t1 :: ATm  t2 :: ATm
  | ALam   x :: {Var}  t :: ATm

data ATm
  | AExp   e :: AExp
  | ALet   x :: {Var}  e :: AExp t :: ATm
